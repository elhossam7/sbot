{
  "name": "solana-trading-bot",
  "version": "1.0.0",
  "description": "A trading bot for Solana that automates trading strategies and interacts with the Telegram Bot API.",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "ts-node --esm src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "dev": "nodemon --exec ts-node --esm src/index.ts",
    "watch": "tsc -w",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "queries": "prisma migrate dev"
  },
  "dependencies": {
    "@jup-ag/api": "^6.0.12",
    "@prisma/client": "^5.9.1",
    "@pythnetwork/client": "^2.22.0",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.90.0",
    "dotenv": "^16.4.1",
    "telegraf": "^4.15.3",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "prisma": "^5.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "node --experimental-specifier-resolution=node --loader ts-node/esm prisma/seed.ts"
  },
  "keywords": [
    "solana",
    "trading",
    "bot",
    "telegram"
  ],
  "author": "Hossam El Mrabti",
  "license": "MIT"
}
